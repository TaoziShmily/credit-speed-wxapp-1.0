view {
  box-sizing: border-box;
}
button:after {
  display: none;
  border-radius: 0;
}
video {
  width: 100%;
  display: block;
}
image {
  display: block;
}
.m-line-h2 {
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
}
.m-line-h3 {
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 3;
  -webkit-box-orient: vertical;
}
.o-line-h {
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
